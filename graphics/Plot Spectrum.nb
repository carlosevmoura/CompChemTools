Cell[TextData[{
 StyleBox["PeakGen",
  FontWeight->"Bold"],
 ": Generate an Gaussian Distribution upon a given transition\n\t#1: \
Amplitude (Oscillator Strength)\n\t#2: Transition Energy\n\t#3: Standard \
Deviation (StDev variable)\n\t#4: Dimension (Recommended: x)"
}], "Subsection",
 InitializationGroup->
  True,ExpressionUUID->"eaf4b377-4b52-4567-be7a-bd93a7114d8b"],

Cell[BoxData[
 RowBox[{"PeakGen", ":=", 
  RowBox[{
   RowBox[{"#1", "*", 
    RowBox[{"PDF", "[", 
     RowBox[{
      RowBox[{"NormalDistribution", "[", 
       RowBox[{"#2", ",", "#3"}], "]"}], ",", "#4"}], "]"}], "*", 
    RowBox[{"(", 
     RowBox[{"#3", "*", 
      SqrtBox[
       RowBox[{"2", "\[Pi]"}]]}], ")"}]}], "&"}]}]], "Input",
 InitializationGroup->
  True,ExpressionUUID->"df585d6c-b483-4548-953f-a43e10fd8022"]
}, Open  ]],

